amp": "2026-02-17T23:00:35.225248"}, {"tool": "workbench", "args": {"action": "create_script", "script_name": "coinmarketcal_events.py", "language": "python", "content": "#!/usr/bin/env python3\n\"\"\"\nCoinMarketCal events fetcher (public API wrapper).\nUsage:\n  python coinmarketcal_events.py --query \"bitcoin\" --days 7\nEnv:\n  CMCAL_API_KEY=your_key\n\"\"\"\nimport os, sys, json, argparse\nimport requests\nfrom datetime import datetime, timezone\n\nAPI_BASE = \"https://developers.coinmarketcal.com/v1\"\n\ndef main():\n    ap = argparse.ArgumentParser()\n    ap.add_argument(\"--query\", default=\"bitcoin\")\n    ap.add_argument(\"--days\", type=int, default=7)\n    ap.add_argument(\"--page\", type=int, default=1)\n    ap.add_argument(\"--max\", type=int, default=20)\n    args = ap.parse_args()\n\n    api_key = os.getenv(\"CMCAL_API_KEY\")\n    if not api_key:\n        print(\"{\\\"ok\\\": false, \\\"error\\\": \\\"CMCAL_API_KEY not set\\\"}\")\n        sys.exit(1)\n\n    headers = {\"x-api-key\": api_key}\n    params = {\n        \"query\": args.query,\n        \"page\": args.page,\n        \"max\": args.max,\n        \"sortBy\": \"date\",\n        \"sortDir\": \"asc\",\n        \"showOnly\": \"upcoming\",\n        \"dateRange\": f\"{args.days}d\",\n    }\n    r = requests.get(f\"{API_BASE}/events\", headers=headers, params=params, timeout=20)\n    r.raise_for_status()\n    data = r.json()\n    # normalize output\n    out = []\n    for ev in data.get(\"body\", []):\n        out.append({\n            \"title\": ev.get(\"title\"),\n            \"date\": ev.get(\"date_event\"),\n            \"coins\": [c.get(\"symbol\") for c in ev.get(\"coins\", [])],\n            \"category\": ev.get(\"categories\"),\n            \"confidence\": ev.get(\"percentage\"),\n            \"source\": ev.get(\"source\")\n        })\n    print(json.dumps({\"ok\": True, \"count\": len(out), \"events\": out}, ensure_ascii=False))\n\nif __name__ == \"__main__\":\n    main()\n"}, "result": "C